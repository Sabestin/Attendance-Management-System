<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.siesattendance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AttendanceActivity"></activity>
        <activity android:name=".AddStudentActivity" />
        <activity android:name=".SubjectList" />
        <activity android:name=".TeacherActivity" />
        <activity android:name=".SelectRollNo" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".StudentInformationActivity" />
        <activity android:name=".StudentLogin" />
        <activity android:name=".TeacherLogin" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>